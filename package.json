{
  "name": "dgg-clicker",
  "version": "0.1.0",
  "scripts": {
    "build": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\"",
    "build-client": "NODE_ENV=production parcel build src/client/index.html --out-dir dist/client",
    "build-server": "tsc -b src/server",
    "test-game": "jest src/game --config src/game/jest.config.js",
    "test-server": "jest src/server --config src/server/jest.config.js",
    "dev": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "dev-client": "node src/client/client-dev-server.js",
    "dev-server": "tsc-watch --noClear -b ./src/server/tsconfig.json --onSuccess \"npm run start\"",
    "start": "ts-node -P src/server/tsconfig.json -r tsconfig-paths/register src/server/lib/index.ts",
    "type-check-client": "tsc -p ./src/client/tsconfig.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migrate": "npm run typeorm migration:run"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@hapi/joi": "^17.1.1",
    "@reach/router": "^1.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.2",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "@use-it/interval": "^0.1.3",
    "antd": "^4.0.0",
    "async": "^3.1.0",
    "axios": "^0.19.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "browser-cookies": "^1.2.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "customize-cra": "^0.9.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "immer": "^5.3.6",
    "jest": "^26.0.1",
    "lerna": "^3.19.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "moment": "^2.24.0",
    "mysql": "^2.14.1",
    "normalize.css": "^8.0.1",
    "parcel-bundler": "^1.12.4",
    "pg": "^7.15.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.13.1",
    "react-spring": "^8.0.27",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "timesync": "^1.0.4",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "0.2.22",
    "typescript": "^3.9.3",
    "uuid": "^3.3.3",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@types/hapi__joi": "^17.1.2",
    "@types/parcel-bundler": "^1.12.1",
    "@types/ws": "^7.2.5",
    "concurrently": "^5.2.0",
    "cssnano": "^4.1.10",
    "http-proxy-middleware": "^1.0.4",
    "tsc-watch": "^4.2.7",
    "tslint": "^5.20.1"
  },
  "_moduleAliases": {
    "@game": "dist/game"
  },
  "alias": {
    "@game": "./src/game"
  }
}
