{
	"name": "dgg-clicker",
	"version": "0.1.0",
	"scripts": {
		"build": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\"",
		"build-client": "NODE_ENV=production parcel build src/client/index.html",
		"build-server": "tsc -b src/server",
		"test-game": "jest src/game --config src/game/jest.config.js",
		"dev": "concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
		"dev-client": "parcel --port 3000 ./src/client/index.html",
		"dev-server": "tsc-watch --noClear -b ./src/server/tsconfig.json --onSuccess \"node --enable-source-maps index.js\"",
		"start": "node index.js",
		"start:prod": "NODE_ENV=production node index.js",
		"type-check-client": "tsc -p ./src/client/tsconfig.json"
	},
	"dependencies": {
		"@emotion/core": "^10.0.28",
		"@emotion/styled": "^10.0.27",
		"@hapi/joi": "^17.1.1",
		"@reach/router": "^1.3.3",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.4.0",
		"@testing-library/user-event": "^7.1.2",
		"@types/classnames": "^2.2.10",
		"@types/hapi__joi": "^16.0.5",
		"@types/jest": "^24.0.24",
		"@types/lodash": "^4.14.149",
		"@types/node": "^13.1.2",
		"@types/reach__router": "^1.3.5",
		"@types/react": "^16.9.17",
		"@types/react-dom": "^16.9.4",
		"@typescript-eslint/eslint-plugin": "^2.12.0",
		"@typescript-eslint/parser": "^2.12.0",
		"@use-it/interval": "^0.1.3",
		"antd": "^4.0.0",
		"async": "^3.1.0",
		"axios": "^0.19.0",
		"babel-jest": "^24.9.0",
		"bluebird": "^3.7.2",
		"body-parser": "^1.19.0",
		"browser-cookies": "^1.2.0",
		"classnames": "^2.2.6",
		"cookie-parser": "^1.4.4",
		"core-js": "^3.6.0",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"customize-cra": "^0.9.1",
		"dotenv": "^8.2.0",
		"eslint": "^6.7.2",
		"express": "^4.17.1",
		"express-http-proxy": "^1.6.0",
		"immer": "^5.3.6",
		"jest": "^24.9.0",
		"lerna": "^3.19.0",
		"less": "^3.11.1",
		"less-loader": "^5.0.0",
		"lodash": "^4.17.15",
		"module-alias": "^2.2.2",
		"moment": "^2.24.0",
		"mysql": "^2.14.1",
		"normalize.css": "^8.0.1",
		"pages": "0.0.16",
		"pg": "^7.15.1",
		"react": "^16.12.0",
		"react-app-rewired": "^2.1.5",
		"react-dom": "^16.12.0",
		"react-scripts": "3.3.0",
		"react-spring": "^8.0.27",
		"redis": "^2.8.0",
		"reflect-metadata": "^0.1.13",
		"shared": "^0.2.0",
